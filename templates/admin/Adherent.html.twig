 <table class="table table-dark">
     <thead class="thead-dark">
         <tr>
             <th scope="col">Nom</th>

             <th scope="col">Prenom</th>
             <th scope="col">dossier</th>
             <th scope="col">FreqenceMensuelle</th>
             <th scope="col">NbPassage</th>
             <th scope="col">Date</th>
             <th scope="col">Adulte</th>
             <th scope="col">Enfant</th>
             <th scope="col">Colis</th>
             <th scope="col">Telephone</th>
             <th scope="col">Payé</th>
             <th scope="col">hygiéne</th>
             <th scope="col">Lessive</th>
             <th scope="col">Couches</th>
             <th scope="col">Année premiere accueil</th>
             <th scope="col">Observation</th>


         </tr>
     </thead>
     {% for Adherent in Adherents %}





     <td>{{ Adherent.Nom}}</td>
     <td>{{ Adherent.Prenom}}</td>
     <td>{{ Adherent.dossier}}</td>
     <td>{{ Adherent.FreqenceMensuelle}}</td>
     <td>{{ Adherent.NbPassage}}</td>
     <td>{{ Adherent.Date}}</td>
     <td>{{ Adherent.Adulte}}</td>
     <td>{{ Adherent.Enfant}}</td>
     <td>{{ Adherent.Colis}}</td>
     <td>{{ Adherent.Telephone}}</td>
     <td>{{ Adherent.Paye}}</td>
     <td>{{ Adherent.hygiene}}</td>
     <td>{{ Adherent.Lessive}}</td>
      <td>{{ Adherent.Couches}}</td>
       <td>{{ Adherent.Annee}}</td>
        <td>{{ Adherent.Observation}}</td>


     <td>{{ projet.datedemande|date('Y-m-d') }} </td>
     <td>{{ projet.daterelance|date('Y-m-d') }} </td>
     <td>{{ projet.dateentretient|date('Y-m-d') }}</td>
     <td
         style="{% if  projet.statut == 'Relancée' %} background-color: #ffd97d; {% elseif  projet.statut == 'Positif' %} background-color: #60d394; {% elseif  projet.statut == 'Négatif' %} background-color: #ee6055; {% endif %}">
         {{ projet.statut }}</td>
     <td><a href="{{ path('editcandidature', {'id' : projet.id}) }}" class="btn btn-warning">modifier</a></td>
     <td><a href="{{ path('delete_candidature', {'id' : projet.id}) }}" class="btn btn-danger">suprimer</a></td>


     </tr>

     </article>



     {% endfor %}
     </tbody>

 </table>





 {% endblock %}